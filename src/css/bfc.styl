// 废弃
[bfc~='l'] {
  float: left;
}

[bfc~='r'] {
  float: right;
}

[bfc~='cl'] {
  clear: left;
}

[bfc~='cr'] {
  clear: right;
}

[bfc~='cb'] {
  clear: both;
}

[bfc~='clear'] {
  clear();
}

[bfc~='db'] {
  display: block;
}

[bfc~='dib'] {
  display: inline-block;
}

[bfc~='dif'] {
  display: inline-flex;
}

[bfc~='dtc'] {
  display: table-cell;
}

[bfc~='pa'] {
  position: absolute;
}

[bfc~='pr'] {
  position: relative;
}

[bfc~='pf'] {
  position: fixed;
}

[bfc~='oh'] {
  overflow: hidden;
}

[bfc~='1'] {
  z-index: 1;
}

[bfc~='9'] {
  z-index: 9;
}

[bfc~='10'] {
  z-index: 10;
}

[bfc~='w100'] {
  width: 100%;
}

[bfc~='h100'] {
  height: 100%;
}