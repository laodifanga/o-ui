// -------------- 间隔 grap="p10 pt10 pr10 pb10 pl10 m10"
// 黄金分割算法 5 / .618 开始 废弃
nums = 2 5 10 15 20 30 40 50
createGaps(n, unit, grapName = 'gap')
  [{grapName}~=p{n}]
    padding n * unit
  [{grapName}~=pt{n}]
    padding-top n * unit
  [{grapName}~=pr{n}]
    padding-right n * unit
  [{grapName}~=pb{n}]
    padding-bottom n * unit
  [{grapName}~=pl{n}]
    padding-left n * unit

  [{grapName}~=m{n}]
    margin n * unit
  [{grapName}~=mt{n}]
    margin-top n * unit
  [{grapName}~=mr{n}]
    margin-right n * unit
  [{grapName}~=mb{n}]
    margin-bottom n * unit
  [{grapName}~=ml{n}]
    margin-left n * unit

for n,i in nums
  createGaps(n, 2px)