@font-face {
  font-family: 'oui';
  src:
    url('fonts/oui.ttf?eoe3fr') format('truetype'),
    url('fonts/oui.woff?eoe3fr') format('woff'),
    url('fonts/oui.svg?eoe3fr#oui') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'oui' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  // line-height: 1;
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

