:root {
	// 主色
	--main-10: #02302b;
	--main: #036d61;
	--main10: #f1f8f7;
	// 橘色
	--orange-10: #E65100;
	--orange: #FFA726;
	--orange10: #FFF3E0;
	// 黄色
	--yellow-10: #F57F17;
	--yellow: #FFEB3B;
	--yellow10: #FFFDE7;
	// 青橙色
	--lime-10: #5F8000;
	--lime: #AEEA00;
	--lime10: #F9FBE9;
	// 绿色
	--green-10: #2B662E;
	--green: #3D9241;
	--green10: #F1F8E9;
	// 蓝绿色
	--teal-10: #2A645E;
	--teal: #26A69A;
	--teal10: #E0F2F1;
	// 青色
	--cyan-10: #007887;
	--cyan: #00BCD4;
	--cyan10: #F3FCFD;
	// 蓝色
	--blue-10: #135699;
	--blue: #49A9F5;
	--blue10: #F3F7FF;
	// 紫色
	--purple-10: #8E24AA;
	--purple: #AB47BC;
	--purple10: #F3E5F5;
	// 粉色
	--pink-10: #B21147;
	--pink: #EC407A;
	--pink10: #FCE4EC;
	// 红色
	--red-10: #A01C1C;
	--red: #FF4242;
	--red10: #FFEBEE;

	--silver-10: #C8C8C8;
	--silver: #DCDCDC;
	--silver10: #f5f5f5;

	--gray-10: #787878;
	--gray: #A0A0A0;
	--gray10: #eee;

	--black-10: #000;
	--black: #282828;
	--black10: #777;

	--white: #fff;
}

[color~='main-10'] { color: var(--main-10) }
[color~='main'] { color: var(--main) }
[color~='main10'] { color: var(--main10) }
[color~='orange-10'] { color: var(--orange-10) }
[color~='orange'] { color: var(--orange) }
